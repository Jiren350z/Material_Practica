@model Proyecto3V3_Camilo_P_Diego_M.Models.NúmeroDeTelefonoDePaciente

@{
    ViewData["Title"] = "Crear";
}

<h1>Crear</h1>

<h4>Número De Telefono De Paciente</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdnumPac" class="control-label"></label>
                <select asp-for="IdnumPac" class ="form-control" asp-items="ViewBag.IdnumPac"></select>
            </div>
            <div class="form-group">
                <label asp-for="NúmeroDeTelefono" class="control-label"></label>
                <input asp-for="NúmeroDeTelefono" class="form-control" />
                <span asp-validation-for="NúmeroDeTelefono" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
